<!-- include header footer -->
<%- include('../header.ejs') %>
    <br>
    <br>
    <br>
    <h1>course display</h1>
    <div class="container">
        <table class="table table-bordered">
            <% if(message.length > 0){ %>
                <div class="alert alert-success">
                  <%= message %>
                </div>
              <% } %>  
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Address</th>
                 
            </tr>
            <% d.forEach((c)=>{ %>
                <tr>
                    <td><%= c._id %></td> 
                    <td><%= c.name %></td>
                    <td><%= c.email %></td>
                    <td><%= c.address %></td>
                     
                    <td>
                        <a href="/courseview/<%= c._id %>" class="btn btn-info">View</a>
                        <a href="/courseedit/<%= c._id %>" class="btn btn-success">Edit</a>
                        <a href="/coursedelete/<%= c._id %>" class="btn btn-danger">Delete</a>
                    </td>
                </tr>

            <%})%>
             

        </table>
    </div>
    <%- include('../footer.ejs') %>